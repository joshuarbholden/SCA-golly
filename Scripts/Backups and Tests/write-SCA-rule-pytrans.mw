<Output><Text-field style="Line Printed Output" layout="Line Printed Output">def setupruletable (crossingrule, turningrule):
    Rules = dict()
    bincrossrule = convert(crossingrule + 512, base, 2)
    binturnrule = convert(turningrule + 512, base, 2)
    states = [[[],&quot;e&quot;],[[&quot;R&quot;],&quot;d&quot;],[[&quot;L&quot;],&quot;d&quot;],[[&quot;L&quot;,&quot;R&quot;],&quot;d&quot;],[[&quot;R&quot;,&quot;L&quot;],&quot;d&quot;],[[&quot;R&quot;],&quot;u&quot;],[[&quot;L&quot;],&quot;u&quot;],[[&quot;L&quot;,&quot;R&quot;],&quot;u&quot;],[[&quot;R&quot;,&quot;L&quot;],&quot;u&quot;]]
    for i in states:
        for j in states:
            for k in states:
                if not(in(CodeGeneration:-R, i[0]) or in(L, k[0])):
                    Rules[i - 1, j - 1, k - 1] = []
                elif in(CodeGeneration:-R, i[0]) and notin(L, k[0]):
                    Rules[i - 1, j - 1, k - 1] = [&quot;R&quot;]
                elif notin(CodeGeneration:-R, i[0]) and in(L, k[0]):
                    Rules[i - 1, j - 1, k - 1] = [&quot;L&quot;]
                elif member(i[0], [[&quot;L&quot;,&quot;R&quot;],[&quot;R&quot;],[&quot;R&quot;,&quot;L&quot;]], itrin) and member(k[0], [[&quot;R&quot;,&quot;L&quot;],[&quot;L&quot;],[&quot;L&quot;,&quot;R&quot;]], ktrin):
                    if equal(i[1], &quot;u&quot;):
                        itrin = 2
                    if equal(k[1], &quot;u&quot;):
                        ktrin = 2
                    iktrin = 3 * itrin - 4 + ktrin
                    if iktrin &lt; 0 or 9 &lt; iktrin:
                        raiseerror
                    if bincrossrule[iktrin + 1 - 1] == 0:
                        Rules[i - 1, j - 1, k - 1] = [&quot;L&quot;,&quot;R&quot;]
                    elif bincrossrule[iktrin + 1 - 1] == 1:
                        Rules[i - 1, j - 1, k - 1] = [&quot;R&quot;,&quot;L&quot;]
                    else:
                        raiseerror
                else:
                    raiseerror
                if Rules[i - 1, j - 1, k - 1] == []:
                    Rules[i - 1, j - 1, k - 1] = [Rules[i - 1, j - 1, k - 1],&quot;e&quot;]
                elif member(i[1], [&quot;u&quot;,&quot;e&quot;,&quot;d&quot;], itrin) and member(k[1], [&quot;u&quot;,&quot;e&quot;,&quot;d&quot;], ktrin):
                    if equal(i[0], []):
                        itrin = 2
                    if equal(k[0], []):
                        ktrin = 2
                    iktrin = 3 * itrin - 4 + ktrin
                    if iktrin &lt; 0 or 9 &lt; iktrin:
                        raiseerrorerror
                    if binturnrule[iktrin + 1 - 1] == 0:
                        if Rules[i - 1, j - 1, k - 1] == [&quot;R&quot;]:
                            Rules[i - 1, j - 1, k - 1] = [[&quot;L&quot;],&quot;u&quot;]
                        elif Rules[i - 1, j - 1, k - 1] == [&quot;L&quot;]:
                            Rules[i - 1, j - 1, k - 1] = [[&quot;R&quot;],&quot;u&quot;]
                        else:
                            Rules[i - 1, j - 1, k - 1] = [Rules[i - 1, j - 1, k - 1],&quot;u&quot;]
                    elif binturnrule[iktrin + 1 - 1] == 1:
                        Rules[i - 1, j - 1, k - 1] = [Rules[i - 1, j - 1, k - 1],&quot;d&quot;]
                    else:
                        raiseerrorerror
                else:
                    raiseerror
    return(Rules)
