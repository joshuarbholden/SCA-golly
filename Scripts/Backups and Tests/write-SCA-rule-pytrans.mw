</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" inline-output="false" labelreference="L2317" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion1" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">setupruletable:= proc(crossingrule, turningrule)
  local i, j, k, bincrossrule, binturnrule, itrin, ktrin, iktrin, Rules, states;
  Rules:=dict();
  bincrossrule:=convert(crossingrule+2^9, base, 2);
  binturnrule:=convert(turningrule+2^9, base, 2);
  states := [[[], &quot;e&quot;], [[&quot;R&quot;], &quot;d&quot;], [[&quot;L&quot;], &quot;d&quot;], [[&quot;L&quot;, &quot;R&quot;], &quot;d&quot;], [[&quot;R&quot;, &quot;L&quot;], &quot;d&quot;], [[&quot;R&quot;], &quot;u&quot;], [[&quot;L&quot;], &quot;u&quot;], [[&quot;L&quot;, &quot;R&quot;], &quot;u&quot;], [[&quot;R&quot;, &quot;L&quot;], &quot;u&quot;]];
  for i in states do
    for j in states do
      for k in states do
         if not(R in i[1]) and not(L in k[1]) then 
              Rules[i, j, k]:=[] #nothing pointing into the cell
         elif (R in i[1]) and not(L in k[1]) then
              Rules[i, j, k]:=[&quot;R&quot;] #only right pointing into the cell
         elif not(R in i[1]) and (L in k[1]) then
              Rules[i, j, k]:=[&quot;L&quot;] #only left pointing into the cell        
         elif member(i[1], [[&quot;L&quot;, &quot;R&quot;], [&quot;R&quot;], [&quot;R&quot;, &quot;L&quot;]], 'itrin') and member(k[1], [[&quot;R&quot;, &quot;L&quot;], [&quot;L&quot;], [&quot;L&quot;, &quot;R&quot;]], 'ktrin') then #both pointing into the cell, set itrin to 1, 2, 3 depending on left, ktrin to 1, 2, 3 depending on right, deal with crossing first
              if equal(i[2],&quot;u&quot;) then itrin := 2 end if;
              if equal(k[2],&quot;u&quot;) then ktrin := 2 end if;
              iktrin:=(itrin-1)*3+(ktrin-1); #0 to 8
              if (iktrin&lt;0) or (iktrin&gt;=9) then raiseerror end if;
             if (bincrossrule[iktrin+1]=0) then Rules[i, j, k]:=[&quot;L&quot;, &quot;R&quot;]
              elif (bincrossrule[iktrin+1]=1) then Rules[i, j, k]:=[&quot;R&quot;, &quot;L&quot;]
              else raiseerror
            end if
         	else raiseerror
         	end if;
         	#start dealing with turning
        if Rules[i, j, k]=[] then 
              Rules[i, j, k]:=[Rules[i, j, k], &quot;e&quot;];
       elif member(i[2], [&quot;u&quot;, &quot;e&quot;, &quot;d&quot;], itrin) and member(k[2], [&quot;u&quot;, &quot;e&quot;, &quot;d&quot;], ktrin) then
              if equal(i[1], []) then itrin := 2 end if;
              if equal(k[1], []) then ktrin := 2 end if;
       	      iktrin:=(itrin-1)*3+(ktrin-1);
              if (iktrin&lt;0) or (iktrin&gt;9) then raiseerrorerror end if;
              if (binturnrule[iktrin+1]=0) then 
                if Rules[i, j, k]=[&quot;R&quot;] then Rules[i, j, k]:=[[&quot;L&quot;], &quot;u&quot;];
                elif Rules[i, j, k]=[&quot;L&quot;] then Rules[i, j, k]:=[[&quot;R&quot;], &quot;u&quot;];
                else Rules[i, j, k]:=[Rules[i, j, k], &quot;u&quot;]
                end if;
             elif (binturnrule[iktrin+1]=1) then Rules[i, j, k]:=[Rules[i, j, k], &quot;d&quot;]
             else raiseerrorerror
             end if
         else raiseerror
         end if;
      end do
    end do
  end do;
return(Rules)
end proc:

