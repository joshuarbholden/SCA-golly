<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">setupcelltodigittable:= proc()
  local celltodigit;
  celltodigit:=table();
  celltodigit[[[], e]]:=0;
  celltodigit[[[L], u]]:=1;
  celltodigit[[[R], u]]:=2;
  celltodigit[[[L,R], u]]:=3;
  celltodigit[[[R,L], u]]:=4;
#  celltodigit[[[], e]]:=5;
  celltodigit[[[L], d]]:=6;
  celltodigit[[[R], d]]:=7;
  celltodigit[[[L,R], d]]:=8;
  celltodigit[[[R,L], d]]:=9;
  return(celltodigit);
end proc:

setupdigittocelltable:= proc()
  local digittocell;
  digittocell:=table();
  digittocell[0]:=[[], e];
  digittocell[1]:=[[L], u];
  digittocell[2]:=[[R], u];
  digittocell[3]:=[[L,R], u];
  digittocell[4]:=[[R,L], u];
  digittocell[5]:=[[], e];
  digittocell[6]:=[[L], d];
  digittocell[7]:=[[R], d];
  digittocell[8]:=[[L,R], d];
  digittocell[9]:=[[R,L], d];
  return(digittocell);
end proc:
