<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">setupruletable:= proc(rulenumber1, rulenumber2)
  local i, j, k, binrule1, binrule2, itrin, ktrin, iktrin, Rules, states;
  Rules:=table();
  binrule1:=convert(rulenumber1+2^9, base, 2);
  binrule2:=convert(rulenumber2+2^9, base, 2);
  states := [[[], e], [[R], d], [[L], d], [[L, R], d], [[R, L], d], [[R], u], [[L], u], [[L, R], u], [[R, L], u]];
  for i in states do
    for j in states do
      for k in states do
         if not(R in i[1]) and not(L in k[1]) then 
              Rules[i, j, k]:=[]
         elif (R in i[1]) and not(L in k[1]) then
              Rules[i, j, k]:=[R]
         elif not(R in i[1]) and (L in k[1]) then
              Rules[i, j, k]:=[L]
         elif member(i[1], [[L, R], [R], [R, L]], 'itrin') and member(k[1], [[R, L], [L], [L, R]], 'ktrin') then
              if i[2] = u then itrin := 2 end if;
              if k[2] = u then ktrin := 2 end if;
              iktrin:=(itrin-1)*3+(ktrin-1);
              if (iktrin&lt;0) or (iktrin&gt;9) then error end if;
              if (binrule1[iktrin+1]=0) then Rules[i, j, k]:=[L, R]
              elif (binrule1[iktrin+1]=1) then Rules[i, j, k]:=[R, L]
              else error
              end if
         else error
         end if;
         if Rules[i, j, k]=[] then 
              Rules[i, j, k]:=[Rules[i, j, k], e]
         elif member(i[2], [u, e, d], 'itrin') and member(k[2], [u, e, d], 'ktrin') then
              if i[1] = [] then itrin := 2 end if;
              if k[1] = [] then ktrin := 2 end if;
              iktrin:=(itrin-1)*3+(ktrin-1);
              if (iktrin&lt;0) or (iktrin&gt;9) then error end if;
              if (binrule2[iktrin+1]=0) then 
                if Rules[i, j, k]=[R] then Rules[i, j, k]:=[[L], u];
                elif Rules[i, j, k]=[L] then Rules[i, j, k]:=[[R], u];
                else Rules[i, j, k]:=[Rules[i, j, k], u]
                end if;
              elif (binrule2[iktrin+1]=1) then Rules[i, j, k]:=[Rules[i, j, k], d]
              else error
              end if
         else error
         end if;
      end do
    end do
  end do;
return(Rules)
end proc:
</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false">fillrows:= proc(Rules, firstrow, numrows)
  local i, j, width, allrows;
  allrows:= table();
  width:=nops(firstrow);
  for j from 0 to width-1 do
      allrows[1, j]:=firstrow[j+1];
  end do;
  for i from 2 to numrows do
    for j from 0 to width-1 do
      allrows[i, j]:=Rules[allrows[i-1, j-1 mod width], allrows[i-1, j], allrows[i-1, j+1 mod width]];
    end do;
  end do;
  return(allrows);
end proc:

drawcell:= proc(i, j, contents)
  local theplot;
  theplot:=[plottools:-line([i,j], [i+1, j], color=gray), plottools:-line([i+1,j+1], [i, j+1], color=gray)];
  if (i+j) mod 2 = 0 then theplot:=[op(theplot), plottools:-line([i+0.5, j], [i+0.5, j+1], color=gray)] end if;
  if contents=[[L], d] then theplot:=[op(theplot), plottools:-line([i+1, j], [i, j+1])]
    elif contents=[[R], d] then theplot:=[op(theplot), plottools:-line([i, j], [i+1, j+1])]
    elif contents=[[L,R], d] then theplot:=[op(theplot), plottools:-line([i+1, j], [i, j+1]), plottools:-line([i, j], [i+.3, j+.3]), plottools:-line([i+.7, j+.7], [i+1, j+1])]
    elif contents=[[R,L], d] then theplot:=[op(theplot), plottools:-line([i, j], [i+1, j+1]), plottools:-line([i+1, j], [i+.7, j+.3]), plottools:-line([i+.3, j+.7], [i, j+1])] 
    elif contents=[[L], u] then theplot:=[op(theplot), plottools:-line([i, j], [i, j+1])]
    elif contents=[[R], u] then theplot:=[op(theplot), plottools:-line([i+1, j], [i+1, j+1])]
    elif contents=[[L,R], u] then theplot:=[op(theplot), plottools:-line([i, j], [i, j+1]), plottools:-line([i+1, j], [i+1, j+1])]
    elif contents=[[R,L], u] then theplot:=[op(theplot), plottools:-line([i, j], [i, j+1]), plottools:-line([i+1, j], [i+1, j+1])] 
  end if;
  return(theplot)
end proc:
  
drawgrid:=proc(rowtable, width, numrows)
  local i, j, theplot;
  theplot:=[];
  for i from 1 to numrows do
    for j from 0 to width-1 do
      theplot:=[op(theplot), op(drawcell(j, i, rowtable[i,j]))];
    end do;
    theplot:=[op(theplot), op(drawcell(-1, i, rowtable[i,width-1]))];
  end do;
  plots:-display(theplot, scaling=constrained, axes=none, view=[-0.5..width-0.5, 0..numrows+1]);
end proc:

drawrows:=proc(rulenum1, rulenum2, firstrow, numrows)
   local grid, Rules;
   Rules:=setupruletable(rulenum1, rulenum2);
   grid:=fillrows(Rules, firstrow, numrows);
   drawgrid(grid, nops(firstrow), numrows);
end proc:
